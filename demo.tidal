d1
  $ stutWith 2 (0.125*4) (stutWith 2 (0.0625) (|* gain 0.95))
  $ s "~ notes" # n "<1 0>"

d2 
  $ s "toys*2" # n "1 3" # speed "<0.5 -0.75 -0.5 0.75 -0.25>" 

d3 
  $ s "[glitch2*2 ~ print*2 glitch*2]*2" # n (irand 16) # speed "<1 1.5 0.5>"

d4
  $ sometimes(# speed "2") $ s "bd*4" # n "1"

d5
  $ s "[supersaw*8 ~]" # n "-26" 

d1 silence

d2 silence

d3 silence

d4 silence

d5 silence

